FoamFile
{
    format      ascii;
    class       dictionary;
    location    "100/uniform";
    object      fieldAverage(U,p,prime2Mean=yes)Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    totalIter       10001;
    totalTime       100.01;
}

p
{
    totalIter       10001;
    totalTime       100.01;
}



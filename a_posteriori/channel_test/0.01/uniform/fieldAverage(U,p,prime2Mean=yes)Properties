FoamFile
{
    format      ascii;
    class       dictionary;
    location    "0.01/uniform";
    object      fieldAverage(U,p,prime2Mean=yes)Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    totalIter       2;
    totalTime       0.02;
}

p
{
    totalIter       2;
    totalTime       0.02;
}



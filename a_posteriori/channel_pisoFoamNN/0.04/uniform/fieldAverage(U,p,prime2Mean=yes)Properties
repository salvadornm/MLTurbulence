FoamFile
{
    format      ascii;
    class       dictionary;
    location    "0.04/uniform";
    object      fieldAverage(U,p,prime2Mean=yes)Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    totalIter       3;
    totalTime       0.06;
}

p
{
    totalIter       3;
    totalTime       0.06;
}


